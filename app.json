{
  "expo": {
    "name": "IRUWA",
    "slug": "IRUWA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#000000",
      "androidMode": "default"
    },
    "android": {
      "package": "com.Line2.iruwa",
      "versionCode": 1,
      "useNextNotificationsApi": true
    },
    "ios": {
      "bundleIdentifier": "com.Line2.iruwa",
      "buildNumber": "1.0.0",
      "supportsTablet": true
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}