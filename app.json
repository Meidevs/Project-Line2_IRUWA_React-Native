{
  "expo": {
    "name": "IRUWA",
    "slug": "IRUWA",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "notification": {
      "icon": "./assets/icon.png",
      "color": "#000000",
      "androidMode": "default"
    },
    "android": {
      "package": "com.Line2.Iruwa",
      "versionCode": 1,
      "useNextNotificationsApi": true,
      "permissions" : [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "FOREGROUND_SERVICE"
      ],
      "googleServicesFile": "./google-services.json",
      "config" : {
        "googleMaps" : {
          "apiKey" : "AIzaSyBZ3D0BWNA_ikCXp4eLXMHq_xJsKiCHtiM"
        }
      }
    },
    "ios": {
      "bundleIdentifier": "com.Line2.Iruwa",
      "buildNumber": "1.0.0",
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": [
          "location",
          "fetch"
        ]
      }
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}